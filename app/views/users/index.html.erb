<h2>Users</h2>
<ul id="user-list">
	<% @users.each do |user| %>
		<li>
			<%= image_tag user.avatar.url(:thumb) %>
			<%= link_to user.name, user %>
		</li>
	<% end %>
</ul>


<% content_for :sidebar do %>
	<%= link_to "Project Home", root_path, :class => "button" %>
	<%= link_to "Add a user", new_user_path, :class => "button" %>
<% end %>
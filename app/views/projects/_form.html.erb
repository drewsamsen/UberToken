<%= form_for(@project) do |f| %>
<%= f.error_messages %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
  	<%= hidden_field_tag "project[user_ids][]", nil %>
  	<% User.all.each do |user| %>
  		<%= check_box_tag "project[user_ids][]", user.id, @project.user_ids.include?(user.id) %>
  		<%= user.name %><br>
  	<% end %>
  </p>
  <%= f.submit %>
<% end %>